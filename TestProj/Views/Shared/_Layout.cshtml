
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Test Project</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Scripts/validation.js")
</head>
<body>
    <nav class="navbar navbar-fixed-top z-depth-1" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand waves-effect waves-light" href="#">© Test Project</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="/Home/Index/" class="panel-heading waves-effect waves-light">Home</a></li>
                <li><a href="/Home/CreateContact/" class="panel-heading waves-effect waves-light">Create new contact</a></li>
            </ul>
        </div>

    </nav>

<div class="content">
    @RenderBody()

</div>
    <!-- JQuery -->
    @Scripts.Render("~/Scripts/material/jquery.min.js")
    <!-- Bootstrap core JavaScript -->
    @Scripts.Render("~/Scripts/material/bootstrap.min.js")
    <!-- Material Design Bootstrap -->
    @Scripts.Render("~/Scripts/material/mdb.js")
@RenderSection("scripts", required: false)
</body>
</html>
